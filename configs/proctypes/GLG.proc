group_details:
  - id: noise_dwn_inhib
    node_details:
      - node_type: plate
        pct_of_group: 1
    pct_of_stage: 0.5
  - id: noise_adj_inhib
    node_details:
      - node_type: plate
        pct_of_group: 1
    pct_of_stage: 0.5
  - id: cycle_gate_ctrl
    node_details:
      - node_type: plate
        pct_of_group: 1
    pct_of_stage: 0.5
  - id: cycle_stg_adv
    node_details:
      - node_type: plate
        pct_of_group: 1
    pct_of_stage: 0.5
  - id: cntxt_stg_adv
    node_details:
      - node_type: plate
        pct_of_group: 1
    pct_of_stage: 0.5
  - id: cntxt_up_inhib
    node_details:
      - node_type: plate
        pct_of_group: 1
