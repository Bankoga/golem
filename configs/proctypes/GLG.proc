  cycle_gate_ctrl: [cycle_relay-energy,noise_dwn_inhib-inhibitor]
  cycle_stg_adv: [cycle_relay-energy]
  cntxt_stg_adv: [cntxt_relay-energy]
  cntxt_up_inhib: [cntxt_relay-energy]
  relay_stg_adv: null
