  noise_adj_inhib: [noise_dwn_inhib-inhibitor]
  cycle_gate_ctrl: [cycle_relay-inhibitor]
  cycle_stg_adv: [cycle_relay-energy,cntxt_ctrl-energy,proc_ctrl-energy]
  cntxt_stg_adv: [cntxt_relay-energy,cntxt_ctrl-energy,proc_ctrl-energy]
  cntxt_up_inhib: [cntxt_relay-inhibitor]
  relay_stg_adv: null
hooks_into:
  noise_in: [noise_dwn_inhib,noise_adj_inhib]
  cycle_in: [cycle_gate_ctrl,cycle_stg_adv]
  cntxt_in: [cntxt_stg_adv,cntxt_up_inhib]
  relay_in: [relay_stg_adv]
hooks_outof:
    cycle_out: [cycle_stg_adv]
    cntxt_out: [cntxt_stg_adv]
    relay_out: [relay_stg_adv]
links_defined:
  - id: loop_i
    hooks_required:
      - cntxt_in
      - cntxt_out
      - cycle_in
      - cycle_out
      - noise_in
      - noise_out
  - id: gate_i
    hooks_required:
