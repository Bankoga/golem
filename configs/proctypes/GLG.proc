    total_count: 2
  - id: cycle_relay
    groups: [cycle_gate_ctrl,cycle_stg_adv]
    total_count: 2
  - id: cntxt_relay
    groups: [cntxt_stg_adv,cntxt_up_inhib]
    total_count: 2
  - id: relay
    groups: [relay_stg_adv]
